{
  "name": "my-extension",
  "version": "0.0.1",
  "description": "A Cognigy.AI Extension",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz $npm_package_name-$npm_package_version.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Cognigy.AI",
    "extensions"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@cognigy/extension-tools": "^0.16.1"
  },
  "devDependencies": {
    "@types/node": "^13.13.15",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
